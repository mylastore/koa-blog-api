{
  "name": "koa-api",
  "version": "2.0.0",
  "description": "Back end for mylastore client",
  "author": "Oscar Quinteros",
  "private": true,
  "scripts": {
    "start": "NODE_ENV=development nodemon app.js --exec",
    "test": "NODE_ENV=testing jest --forceExit",
    "build": "NODE_ENV=production babel src -d build && npm run pretty",
    "pretty": "prettier --write --print-width 80 --single-quote --trailing-comma es5 --tab-width 4 --no-semi 'src/**/*.js' 'tests/**/*.js'",
    "production": "NODE_ENV=production pm2 start ecosystem.json",
    "live": "NODE_ENV=production node app.js",
    "clean": "npm cache clean --force",
    "seed": "node -r esm src/seed/seed.js"
  },
  "dependencies": {
    "@sendgrid/mail": "7.3.0",
    "bcrypt": "5.0.0",
    "dotenv": "6.0.0",
    "esm": "3.2.25",
    "express-session": "1.17.0",
    "faker": "4.1.0",
    "formidable": "1.2.2",
    "google-auth-library": "6.1.3",
    "ioredis": "3.2.2",
    "jsonwebtoken": "8.2.2",
    "kcors": "2.2.1",
    "koa": "2.13.0",
    "koa-body": "4.2.0",
    "koa-helmet": "5.2.0",
    "koa-json-error": "3.1.2",
    "koa-qs": "3.0.0",
    "koa-ratelimit": "4.1.1",
    "koa-router": "7.4.0",
    "koa-static": "5.0.0",
    "koa-useragent": "1.1.0",
    "log4js": "6.3.0",
    "mongoose": "^5.12.11",
    "mongoose-unique-validator": "2.0.3",
    "node-fetch": "2.6.1",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "pm2": "^4.5.6",
    "randexp": "0.4.9",
    "sharp": "0.26.2",
    "shortid": "2.2.15",
    "slugify": "1.4.5",
    "string-strip-html": "6.0.4"
  },
  "devDependencies": {
    "@node-minify/babel-minify": "5.3.0",
    "@node-minify/core": "6.0.0",
    "@node-minify/uglify-js": "5.3.0",
    "babel-cli": "^6.18.0",
    "babel-polyfill": "6.26.0",
    "babel-preset-env": "1.7.0",
    "babel-register": "6.26.0",
    "cross-env": "6.0.0",
    "jest": "26.4.0",
    "nodemon": "2.0.4",
    "prettier": "1.13.4"
  },
  "babel": {
    "presets": [
      [
        "env",
        {
          "targets": {
            "browsers": [
              "last 2 versions",
              "safari >= 7"
            ]
          }
        }
      ]
    ]
  },
  "jest": {
    "testEnvironment": "node"
  }
}
